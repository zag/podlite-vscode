{
  "name": "podlite",
  "displayName": "Podlite",
  "description": "Podlite AI-Assisted Documentation Plugin for VSCode",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.86.0"
  },
  "author": {
    "name": "Aliaksandr Zahatski",
    "email": "zag@cpan.org"
  },
  "icon": "podlite_logo_256x256.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/zag/podlite-vscode.git"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "publisher": "podlite",
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "podlite",
        "aliases": [
          "Podlite",
          "PodLite"
        ],
        "extensions": [
          ".podlite",
          ".podl",
          ".pod6",
          ".rakudoc"
        ]
      }
    ],
    "commands": [
      {
        "command": "podlite.ai.createDoc",
        "title": "Podlite âœ¨: Create Doc"
      },
      {
        "command": "podlite.openSettings",
        "title": "Podlite: open settings"
      },
      {
        "command": "podlite.preview",
        "title": "Toggle Preview",
        "category": "Podlite"
      },
      {
        "command": "podlite.previewToSide",
        "title": "Open Preview to the Side",
        "category": "Podlite"
      }
    ],
    "configuration": {
      "title": "Podlite - documenting your code with AI",
      "properties": {
        "podlite.ai.openAI.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key (Default: '')",
          "title": "API Key",
          "patternErrorMessage": "Invalid API Key format. Please enter a valid API Key.",
          "markdownDescription": "API Key is required to authenticate and access the [OpenAI Platform](https://platform.openai.com/account/api-keys)."
        },
        "podlite.ai.openAI.engine": {
          "type": "string",
          "default": "gpt-4-turbo-preview",
          "description": "AI Model (Default: 'gpt-3.5-turbo-1106')",
          "title": "AI Model",
          "enum": [
            "text-embedding-3-small",
            "gpt-4-turbo-preview",
            "gpt-3.5-turbo-1106"
          ],
          "markdownDescription": "The AI Model determines the behavior and capabilities of the language model."
        },
        "podlite.ai.openAI.docPrompt": {
          "type": "string",
          "default": "Generate documentation for the following {lang} function {functionText}. Do not add any other strings beside the function and its documentation. Include data about the params and the return, and also a description about the function, do not include examples.{note}",
          "description": "Use the following keys to customize the prompt: {lang} - lang of sources , {functionText} - selected text, {note} - additional notes"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "_compile_prveiw": "npx esbuild@0.21.4 src/webview/index.tsx --sourcemap --bundle --outfile=out/index.js --external:vscode --format=cjs --platform=browser --minify",
    "compile_prveiw": "npx esbuild@0.21.4 src/webview/index.tsx --sourcemap --bundle --outfile=out/index.js --external:vscode --format=cjs --platform=browser",
    "webpack-frontend-dev": "webpack --mode development --watch --config ./webpack.frontend.config.js",
    "prewebpack": "webpack --mode development --config ./webpack.frontend.config.js",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "clean": "rimraf dist",
    "format": "npx prettier --write '**/*.{js,jsx,ts,tsx,json,md,mjs}'"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18",
    "@types/react-dom": "^18.2.18",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^8.56.0",
    "podlite": "^0.0.42",
    "rimraf": "^5.0.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@podlite/editor-react": "^0.0.35",
    "@podlite/markdown": "^0.0.18",
    "@podlite/to-jsx": "^0.0.29",
    "css-loader": "^7.1.2",
    "esbuild": "0.21.4",
    "openai": "^4.28.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^4.0.0"
  },
  "resolutions": {
    "podlite": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/core",
    "@podlite/diagram": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-diagrams",
    "@podlite/editor-react": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-editor-react",
    "@podlite/formula": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-formula",
    "@podlite/image": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-image",
    "@podlite/markdown": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-markdown",
    "@podlite/publisher": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-publisher",
    "@podlite/schema": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-schema",
    "@podlite/to-jsx": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-to-jsx",
    "@podlite/toc": "portal:/Users/zag/Dropbox/Work/proj/podlite/packages/podlite-toc"
  }
}
